<%= javascript_include_tag("//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js") %>
<%= javascript_include_tag("jquery.tubular.1.0.js") %>
<%= javascript_include_tag("chessboard") %>

<%= stylesheet_link_tag 'chessboard' %>

<body>
<div id="wrapper">
  <button id="set_background" class="btn btn-danger btn-xs" disabled>
    set background
  </button><br />
  <button type="button" class="btn btn-danger" id="hide_button">
    Hide/Show
  </button>
  <div id="hide_content_top">
    <% if current_user %>
      <h1><%= current_user.first_name.titlecase %>'s Profile</h1>
      <%= render 'index_content' %>
    <% else %>
      <h1>Sample Profile</h1>
      <%= render 'index_content' %>
    <% end %>
</body>
